{
  "name": "vue-test-attribute",
  "version": "1.3.0",
  "description": "Vue 2 directive that adds data-test attribute to elements",
  "main": "commonjs/index.js",
  "module": "es/index.js",
  "scripts": {
    "test": "npm run test:main && npm run test:ssr && npm run test:prod-flag",
    "test:main": "jest __tests__/main.js",
    "test:ssr": "jest __tests__/ssr.js",
    "test:prod-flag": "jest __tests__/production-flag-true.js && jest __tests__/production-flag-false.js && jest __tests__/production-flag-not-set.js",
    "prebuild": "npm run clean",
    "build": "npm run build:commonjs && npm run build:es",
    "build:commonjs": "BABEL_ENV=commonjs babel index.js --out-dir commonjs",
    "build:es": "BABEL_ENV=es babel index.js --out-dir es",
    "clean": "rimraf es commonjs",
    "prepublishOnly": "npm run test && npm run build"
  },
  "homepage": "https://github.com/psalaets/vue-test-attribute",
  "repository": {
    "type": "git",
    "url": "https://github.com/psalaets/vue-test-attribute.git"
  },
  "files": [
    "commonjs",
    "es"
  ],
  "keywords": [
    "vue",
    "test",
    "attribute"
  ],
  "author": "Paul Salaets <psalaets@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^21.2.1",
    "jest-serializer-vue": "^0.3.0",
    "rimraf": "^2.6.2",
    "vue": "^2.5.9",
    "vue-server-renderer": "^2.5.9",
    "vue-template-compiler": "^2.5.9",
    "vue-test-utils": "^1.0.0-beta.6"
  }
}
